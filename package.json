{
  "name": "express-versioned-controllers",
  "version": "1.0.0",
  "description": "Express middleware for resolving controllers with api versioning support.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build-scripts": "tsc --project build",
    "build": "npm run build-scripts && npm run generate && tsc",
    "build-publish": "npm run build-scripts && npm run generate && tsc --p tsconfig.publish.json",
    "build-tests": "tsc --project test",
    "remove-d": "git clean -xf",
    "generate": "node ./build/declaration-generator.js",
    "test": "mocha test/specs/**/*.js",
    "lint": "tslint .",
    "cover": "nyc --all --require source-map-support/register mocha test/specs/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobinBuschmann/express-controllers.git"
  },
  "author": "Robin Buschmann",
  "license": "ISC",
  "devDependencies": {
    "@types/bluebird": "3.5.2",
    "@types/chai": "3.4.34",
    "@types/express": "4.0.34",
    "@types/mocha": "2.2.35",
    "@types/reflect-metadata": "0.0.5",
    "bluebird": "3.5.0",
    "chai": "3.5.0",
    "codecov": "2.1.0",
    "express": "4.14.0",
    "mocha": "3.2.0",
    "nyc": "10.2.0",
    "reflect-metadata": "0.1.10",
    "source-map-support": "0.4.14",
    "supertest": "2.0.1",
    "ts-type-info": "6.2.3",
    "tslint": "4.1.1",
    "typescript": "2.1.4"
  },
  "dependencies": {
    "@types/pluralize": "0.0.27",
    "es6-shim": "0.35.3",
    "pluralize": "3.1.0"
  },
  "nyc": {
    "lines": 85,
    "statements": 85,
    "functions": 85,
    "branches": 85,
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "test/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "report-dir": "./coverage"
  }
}
